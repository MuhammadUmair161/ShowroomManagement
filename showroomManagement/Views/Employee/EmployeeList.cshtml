@model showroomManagement.Models.Employee

<table class="table table-hover">
    <thead>
        <tr>

            @*<th>Image</th>*@

            <th>FirstName</th>
            <th>LastName</th>
            <th>Email</th>
            <th>Cnic</th>
            <th>Contact</th>
            <th>Address</th>
            <th>Qualification</th>
            <th>DateOfBirth</th>
            <th>Department</th>

        </tr>
    </thead>
    <tbody>
            <tr>
                @*<td><img src="@item.ImagePath" style="width:50px;height:50px;" class="rounded-circle border-danger border-5" /></td>*@

                <td>@Model.FirstName</td>
                <td>@Model.LastName</td>
                <td>@Model.Email</td>
                <td>@Model.Cnic</td>
                <td>@Model.Contact</td>
                <td>@Model.Address</td>
                <td>@Model.Qualification</td>
                <td>@Model.DateOfBirth</td>
                <td>@Model.Department</td>
                <td>
                    <a class="btn btn-primary" asp-route-id="@Model.Id" asp-action="update">update</a>
                    <a class="btn btn-danger" asp-route-id="@Model.Id" asp-action="Delete">Delete</a>
                </td>
            </tr>
    </tbody>
</table>